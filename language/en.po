msgstr "Awareness of security and compliance"

msgid "__category2"
msgstr "Employee management"

msgid "__category3"
msgstr "Third-party management"

msgid "__category4"
msgstr "Physical and environment security"

msgid "__category5"
msgstr "Access control"

msgid "__category6"
